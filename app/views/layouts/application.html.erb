<!DOCTYPE html>
<html>
<head>
  <title>RG</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="header-wrapper">
		<ul id="header">
			<li class="logo"><a href="/"><img id="logo" src="/assets/ResearchGate_Logo.png" alt="ResearchGate_Logo" style="width:178px;height:50px"></a></li>
			<li><a href="/"><img id="home" src="/assets/home.png" alt="ResearchGate_Home" style="width:20px;height:20px"></a></li>
			<li id="qea" ><a href="/questions">Q&amp;A</a></li>
			<li id="publications" ><a href="/publications">Publicações</a></li>
			<li id="jobs" ><a href="/jobs">Trabalhos</a></li>
			<li id="user"><a href="/"><img  src="/assets/user.png" alt="AutentificatedUser" style="width:40px; height:40px"></a></li>
		</ul>
	</div>
	<div class="content">
		<%= yield %>
	</div>

</body>
</html>
